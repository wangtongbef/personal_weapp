.li,
.liResult{
  width: 600rpx;
  height:70rpx;
  display: flex;
  justify-content:space-between;
}
.title,
.operator{
  height:40rpx;
  line-height: 60rpx;
  text-align: right;
  color: #333;
  font-size: 28rpx;
}
.Input,
.earming{
  width:300rpx;
  height:40rpx;
  font-size: 28rpx;
  color:#222;
  text-align: right;
  padding-right: 10rpx;
}
.Input{
  border:1rpx solid #999;
  border-radius: 8rpx;
}
.earming{
  margin-top: 12rpx;
}
.checkType{
  width:310rpx;
  height:40rpx;
  display: flex;
  justify-content: space-between;
}
.typeO,
.typeT{
  width:145rpx;
  height:60rpx;
  background-color: gray;
  border-radius: 8rpx;
  font-size: 28rpx;
  text-align: center;
  line-height: 60rpx
}
.checked{
  background-color: #3299da;
  border:0;
  color:white;
}
.unchecked{
  border:1rpx solid #999;
  background-color: #fff;
  color: #000;
  box-sizing: border-box
}
.title{
  width:200rpx;
}
.operator{
  width:40rpx;
  text-align: left;
}
.confirm{
  width:620rpx;
  height:60rpx;
  line-height: 60rpx;
  text-align: center;
  border-radius: 8rpx;
  font-size: 28rpx;
  margin: 20rpx 0
}
.toSubmit{
  background:#278bc9;
  color:#fff;
}
.noSubmit{
  background:#fff;
  color:#000;
  box-sizing: border-box;
  border:1rpx solid #999;
}
.tabbar{
  width:100%;
  height:100rpx;
  position: fixed;
  bottom: 0;
  background-color: white;
  display: flex;
}
.tabbar view{
  width:calc(100% / 3) ;
  box-sizing: border-box;
  text-align: center;
  line-height: 100rpx;
  font-size: 32rpx;
}
.ranking{
  border-right: 1rpx solid #999;
}